<?xml version="1.0" standalone="yes"?>
<experiment_elements>
  <category>
    <name>Custom UTDallas Sequence Utilities</name>
    <expanded>True</expanded>
    <element>
      <name>Group Data Files using INI</name>
      <classname>GROUPDATAFILESINI</classname>
      <type>normal</type>
      <description>Allows data files generated by the sequencer to be grouped together from an INI File</description>
      <parameters>
        <explain_label tag="INIFILE" value="GAMRY.INI" desc="INI File Name" />
        <explain_label tag="INISECTION" value="GROUPING1" desc="INI Section Name" />
      </parameters>
    </element>
    <element>
      <name>Set INI Flag</name>
      <classname>SETINIFLAG</classname>
      <type>normal</type>
      <description>Allows a flag to be set in an INI File</description>
      <parameters>
        <explain_label tag="INIFILE" value="GAMRY.INI" desc="INI File Name" />
        <explain_label tag="INISECTION" value="FLAGSECTION" desc="INI Section Name" />
        <explain_label tag="FLAGNAME" value="FLAG1" desc="INI Flag Name" />
        <explain_label tag="FLAGVALUE" value="FLAGVALUE" desc="INI Flag Value" />
      </parameters>
    </element>
  </category>
</experiment_elements>